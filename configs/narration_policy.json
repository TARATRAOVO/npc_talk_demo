{
  "max_len": 120,
  "max_sentences": 3,
  "candidates": 3,
  "history_size": 3,
  "focus_cycle": ["visual", "sound", "air", "motion", "light", "object"],
  "relaxed_anonymity": true,
  "allow_names": true,
  "allow_pronouns": true,
  "allow_professions": true,
  "allow_actions": true,
  "require_action": true,
  "action_first": false,
  "enforce_result": true,
  "disable_avoid_list": true,
  "omit_focus": true,
  "no_filter": true,
  "raw_pick": true,
  "disable_fallback": true
}
